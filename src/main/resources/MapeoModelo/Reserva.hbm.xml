<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
            name="ar.edu.unq.epers.aterrizar.model.Reserva" table="reserva">
        <id column="id" name="id" type="integer">
            <generator class="identity"/>
        </id>

		<property name="username" type="string" lazy="true">
            <column name="username" length="45" not-null="false" unique="false"/>
        </property>
        
        <property name="nombreAsiento" type="string" lazy="true">
            <column name="nombreAsiento" length="45" not-null="false" unique="false"/>
        </property>
        
        <property name="tramoOrigen" type="string" lazy="true">
            <column name="tramoOrigen" length="45" not-null="false" unique="false"/>
        </property>
        
        <property name="tramoDestino" type="string" lazy="true">
            <column name="tramoDestino" length="45" not-null="false" unique="false"/>
        </property>
        
        <property column="fechaReserva" generated="never" lazy="false" length="45" name="fechaReserva" type="date"/>
        

    </class>
</hibernate-mapping>