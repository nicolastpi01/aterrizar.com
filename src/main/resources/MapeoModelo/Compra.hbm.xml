<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
            name="ar.edu.unq.epers.aterrizar.model.Compra" table="compra" lazy="true">
        <id column="id" name="id" type="integer">
            <generator class="identity"/>
        </id>

        <property name="username" type="string" lazy="true">
            <column name="username" length="45" not-null="false" unique="false"/>
        </property>
        
        <property name="nombreAsiento" type="string" lazy="true">
            <column name="nombreAsiento" length="45" not-null="false" unique="false"/>
        </property>
        
        <property name="origenTramo" type="string" lazy="true">
            <column name="origenTramo" length="45" not-null="false" unique="false"/>
        </property>
        
        <property name="destinoTramo" type="string" lazy="true">
            <column name="destinoTramo" length="45" not-null="false" unique="false"/>
        </property>

    </class>
</hibernate-mapping>